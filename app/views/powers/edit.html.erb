<h1>Edit this power!<h1>

<%= form_for @power do |f| %>
<%= f.label :name, "Power" %>
<%= f.text_field :name %>
<br>
<%= f.label :description%>
<%= f.text_area :description %>
<br>

<% Heroine.all.each do |heroine| %>
    <%= check_box_tag "power[heroine_ids][]", heroine.id %>
    <%= heroine.super_name %><br>
<% end %>

<%= f.submit %>
<% end %>